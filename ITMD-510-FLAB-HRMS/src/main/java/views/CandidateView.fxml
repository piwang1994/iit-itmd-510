<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" maxHeight="400.0" maxWidth="600.0" prefHeight="400.0"
            prefWidth="600.0" style="-fx-background-color: bbbdbd;"
            xmlns="http://javafx.com/javafx/8.0.171" fx:controller="controllers.CandidateController">
    <children>
        <Button layoutX="180.0" layoutY="32.0" mnemonicParsing="false" onAction="#viewJobs" text="View all jobs"/>
        <Button layoutX="94.0" layoutY="73.0" mnemonicParsing="false" onAction="#getApplist" text="View my apply list"/>
        <Button layoutX="325.0" layoutY="73.0" mnemonicParsing="false" onAction="#createApp" prefHeight="27.0"
                text="Apply for a new job" AnchorPane.rightAnchor="90.0"/>
        <Button layoutX="234.0" layoutY="377.0" mnemonicParsing="false" onAction="#logout" prefHeight="27.0"
                prefWidth="134.0" text="Logout" AnchorPane.bottomAnchor="72.0" AnchorPane.leftAnchor="234.0"
                AnchorPane.rightAnchor="232.0" AnchorPane.topAnchor="377.0"/>
        <TableView fx:id="appList" editable="true" layoutX="187.0" layoutY="138.0" prefHeight="200.0" prefWidth="227.0">
            <columns>
                <TableColumn fx:id="job" prefWidth="50.0" text="job"/>
                <TableColumn fx:id="result" prefWidth="50.0" text="result"/>
            </columns>
        </TableView>
    </children>
</AnchorPane>
